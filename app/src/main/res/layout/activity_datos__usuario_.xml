<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/LytContenedor"
        xmlns:tools="http://schemas.android.com/tools"
        android:fitsSystemWindows="false"
        android:orientation="vertical"
        android:background="@color/md_white_1000"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/statusBar"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:background="?attr/colorPrimary"/>

    <android.support.v7.widget.Toolbar
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="BIENVENIDA"
        android:id="@+id/textView"
        android:textColor="@color/md_pink_500"
        android:textStyle="bold"
        android:layout_gravity="center_horizontal" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="496dp"
        android:gravity="top|center"
        android:layout_marginTop="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="INFORMACION PERSONAL"
            android:id="@+id/textView8"
            android:layout_gravity="left"
            android:textColor="@color/md_pink_500_75" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/NombreTxt"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:textColor="@color/md_black_1000_75"
                android:backgroundTint="@color/md_divider"
                android:layout_gravity="left"
                android:hint="Nombre"
                android:textColorHint="@color/md_disabled_hint_text"
                android:layout_weight="1" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:ems="10"
                android:id="@+id/ApellidoTxt"
                android:backgroundTint="@color/md_divider"
                android:textColor="@color/md_black_1000_75"
                android:layout_gravity="left"
                android:textColorHint="@color/md_disabled_hint_text"
                android:hint="Apellido"
                android:layout_weight="1" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="left|center"
            android:layout_gravity="left">

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="date"
                android:ems="9"
                android:id="@+id/FechaTxt"
                android:backgroundTint="@color/md_divider"
                android:textColor="@color/md_black_1000_75"
                android:textColorHint="@color/md_disabled_hint_text"
                android:hint="Fecha de nacimiento" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Fechabtn"
                android:layout_gravity="right"
                android:background="@android:drawable/ic_menu_my_calendar" />

        </LinearLayout>

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPostalAddress"
            android:ems="10"
            android:id="@+id/Direccion_1Txt"
            android:backgroundTint="@color/md_divider"
            android:textColor="@color/md_black_1000_75"
            android:textColorHint="@color/md_disabled_hint_text"
            android:layout_gravity="left"
            android:hint="Ubicacion" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPostalAddress"
            android:ems="10"
            android:id="@+id/Direccion_2txt"
            android:backgroundTint="@color/md_divider"
            android:textColor="@color/md_black_1000_75"
            android:textColorHint="@color/md_disabled_hint_text"
            android:hint="Direccion"
            android:layout_gravity="left" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:layout_marginTop="3dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="FECHA DE EXAMEN"
                android:id="@+id/textView9"
                android:textColor="@color/md_pink_500_75"
                android:gravity="center_vertical"
                android:layout_weight="2" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView5"
                android:src="@android:drawable/ic_dialog_info"
                android:tint="@color/md_pink_500_75" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="10dp">

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="date"
                android:id="@+id/FechaExamenTxt"
                android:backgroundTint="@color/md_divider"
                android:textColor="@color/md_black_1000_75"
                android:textColorHint="@color/md_disabled_hint_text"
                android:ems="5"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center_horizontal"
                android:hint="Fecha" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/fechaExamenbtn"
                android:layout_gravity="right"
                android:background="@android:drawable/ic_menu_today" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="date"
                android:ems="5"
                android:id="@+id/timetxt"
                android:backgroundTint="@color/md_divider"
                android:textColor="@color/md_black_1000_75"
                android:textColorHint="@color/md_disabled_hint_text"
                android:layout_weight="1"
                android:layout_gravity="center_vertical"
                android:gravity="center_horizontal"
                android:hint="Hora" />

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/time"
                android:layout_gravity="right"
                android:background="@android:drawable/ic_menu_recent_history" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center">

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:gravity="bottom|center|right"
            android:layout_gravity="bottom">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Aceptar"
                android:id="@+id/AceptarBtn"
                android:onClick="clickAceptar" />
        </LinearLayout>

    </LinearLayout>

    <FrameLayout
        android:id="@+id/navBar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/md_pink_500_50"/>

</LinearLayout>
